{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "vottun-angular": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "allowedCommonJsDependencies": [
              "jquery",
              "deepmerge",
              "classnames",
              "css.escape",
              "immutable",
              "lodash",
              "export-to-csv",
              "circletype",
              "prop-types",
              "js-file-download",
              "xml-but-prettier",
              "url-parse",
              "froala-editor",
              "ngx-google-places-autocomplete"
            ],
            "outputPath": "dist/vottun-angular",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",

            "optimization": false,
            "sourceMap": true,
            "namedChunks": true,
            "buildOptimizer": false,

            "assets": [
              "src/favicon.svg",
              "src/assets"
            ],
            "styles": [
              "src/styles.css",
              "src/assets/styles/metisMenu.css",
              "src/assets/styles/animate.css",
              "src/assets/styles/bootstrap.min.css",
              "src/assets/styles/style.css",
              "src/assets/styles/custom_style.css",
              "src/assets/styles/theme-dark.css",
              "node_modules/ngx-smart-modal/styles/ngx-smart-modal.scss",
              "node_modules/frappe-gantt/dist/frappe-gantt.css",
              "node_modules/swagger-ui-dist/swagger-ui.css",
              "./node_modules/quill/dist/quill.snow.css"
            ],
            "scripts": [
            "node_modules/jquery/dist/jquery.min.js",
            "src/assets/js/circletype.min.js",
            "src/assets/js/metisMenu.min.js",
            "src/assets/landing/js/wow.min.js",
            "src/assets/landing/js/particle.js",
            "src/assets/landing/js/bootstrap.min.js",
            "src/assets/js/bootstrap.bundle.min.js",
            "./node_modules/quill/dist/quill.js",
            "./node_modules/quill-image-resize-module/image-resize.min.js",
            "node_modules/moment/min/moment.min.js",
            "node_modules/swagger-ui-dist/swagger-ui-bundle.js",
            "node_modules/swagger-ui-dist/swagger-ui-standalone-preset.js"
            ]
          },
          "configurations": {
            
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": false,
              "outputHashing": "all",
              "sourceMap": true,
              "namedChunks": true,
              "aot": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": false
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "buildTarget": "vottun-angular:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "vottun-angular:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "vottun-angular:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              "styles.css"
            ],
            "scripts": [],
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "vottun-angular-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "vottun-angular:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "warnings": {
      "versionMismatch": false
    },
    "analytics": false
  }
}